#include <iostream>
using namespace std;

int main() {
    int myInt = 30;
    float myFloat = 16.5f;
    double myDouble = 5.86432;
    char myChar = 'A';
    bool myBool = true;

    cout << "int: " << myInt << endl;
    cout << "float: " << myFloat << endl;
    cout << "double: " << myDouble << endl;
    cout << "char: " << myChar << endl;
    cout << "bool: " << myBool << endl;

    return 0;
}
